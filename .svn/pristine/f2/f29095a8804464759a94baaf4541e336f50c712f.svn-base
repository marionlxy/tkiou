  		<select id="queryNavMenuForPage" parameterType="java.util.Map"
		resultType="com.taikang.udp.framework.common.datastructre.impl.BaseDto">

		<![CDATA[ SELECT
		zm.mod_Id AS modId,
		zm.mod_parent_id AS modParentId,
		zm.mod_parent_id AS ParentId,
		zm.MOD_NAME AS modName,
		zm.MOD_ISLEAF AS modIsleaf,
		zm.mod_level AS modLevel,
		zm.MOD_SQUENCE AS modSquence,
		zm.MOD_URL AS modUrl,
		zm.created_time AS createdTime,
		b.menu_name AS menuName,
		zmd.mod_byname AS modByName,
		zmd.TITLE AS title
		FROM
		bsc_menu_t b,
		zjow_module zm,
		zjow_module_des zmd
		WHERE 1=1
		AND zm.MOD_PARENT_ID = b.Menu_Id
		AND zmd.mod_id = zm.mod_id]]>
		<if test=" menuId  != null and  menuId  != ''  and  menuId  != 'null' ">
			AND b.Menu_Id = #{menuId}
		</if>
	</select>

	<select id="querySubMenu" parameterType="java.util.Map"
		resultType="com.taikang.udp.framework.common.datastructre.impl.BaseDto">
		<![CDATA[ SELECT zm.MOD_ID AS modId,zm.MOD_NAME AS modName, zm.MOD_PARENT_ID AS modParentId,zm.MOD_ISLEAF AS modIsleaf,zm.MOD_LEVEL AS modLevel,zm.mod_squence AS modSquence,zm.MOD_URL AS modUrl,zm.CREATED_TIME AS createdTime,zmd.Mod_Name AS modName2,zmd.Title AS tilte FROM zjow_module AS zm, zjow_module_des zmd WHERE 1=1 ]]>
		<if test=" modId  != null and  modId  != ''  and  modId  != 'null' ">
			AND zm.MOD_PARENT_ID =#{modId}
		</if>
		AND zm.MOD_ID=zmd.MOD_ID
		UNION
		SELECT zm.MOD_ID AS modId,zm.MOD_NAME AS
		modName, zm.MOD_PARENT_ID AS modParentId,zm.MOD_ISLEAF AS
		modIsleaf,zm.MOD_LEVEL AS modLevel,zm.mod_squence AS
		modSquence,zm.MOD_URL AS modUrl,zm.CREATED_TIME AS
		createdTime,zmd.Mod_Name AS modName2,zmd.Title AS tilte FROM
		zjow_module AS zm, zjow_module_des zmd WHERE zm.MOD_PARENT_ID IN
		(SELECT zm.MOD_ID FROM zjow_module AS zm WHERE 1=1
		<if test=" modId  != null and  modId  != ''  and  modId  != 'null' ">
			AND zm.MOD_PARENT_ID =#{modId}
		</if>
		)
		AND zm.MOD_ID=zmd.MOD_ID
		UNION
		SELECT zm.MOD_ID AS modId,zm.MOD_NAME AS
		modName, zm.MOD_PARENT_ID AS modParentId,zm.MOD_ISLEAF AS
		modIsleaf,zm.MOD_LEVEL AS modLevel,zm.mod_squence AS
		modSquence,zm.MOD_URL AS modUrl,zm.CREATED_TIME AS
		createdTime,zmd.Mod_Name AS modName2,zmd.Title AS tilte FROM
		zjow_module AS zm, zjow_module_des zmd WHERE zm.MOD_PARENT_ID IN
		(SELECT
		zm.MOD_ID FROM zjow_module AS zm, zjow_module_des zmd WHERE
		zm.MOD_PARENT_ID IN
		(SELECT zm.MOD_ID FROM zjow_module AS zm WHERE 1=1
		<if test=" modId  != null and  modId  != ''  and  modId  != 'null' ">
			AND zm.MOD_PARENT_ID =#{modId}
		</if>
		)
		)
		AND zm.MOD_ID=zmd.MOD_ID
	</select>
